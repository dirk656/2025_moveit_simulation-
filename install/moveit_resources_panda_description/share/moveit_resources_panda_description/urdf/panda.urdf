  <!-- ========================== -->
  <!--        Gazebo 插件         -->
  <!-- ========================== -->
  <gazebo>
    <plugin filename="libgz-sim-joint-trajectory-controller-system.so" name="joint_trajectory_controller">
      <joint_names>panda_joint1,panda_joint2,panda_joint3,panda_joint4,panda_joint5,panda_joint6,panda_joint7</joint_names>
      <initial_position>0,-0.785,0,-2.356,0,1.571,0.785</initial_position>
    </plugin>
  </gazebo>

  <!-- 添加物理引擎配置 -->
  <gazebo>
    <plugin filename="libgz-sim-physics-system.so" name="gz::sim::systems::Physics">
      <engine>bullet</engine>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </plugin>
  </gazebo>

  <!-- 添加重力补偿插件 -->
  <gazebo>
    <plugin filename="libgz-sim-gravity-compensation-system.so" name="gz::sim::systems::GravityCompensation">
      <link_name>panda_link1</link_name>
      <link_name>panda_link2</link_name>
      <link_name>panda_link3</link_name>
      <link_name>panda_link4</link_name>
      <link_name>panda_link5</link_name>
      <link_name>panda_link6</link_name>
      <link_name>panda_link7</link_name>
    </plugin>
  </gazebo>

