      <param name="update_rate">1000</param>
    </hardware>
    
    <!-- 手臂关节 - 添加PID增益 -->
    <joint name="panda_joint1">
      <command_interface name="position">
        <param name="min">-2.9671</param>
        <param name="max">2.9671</param>
      </command_interface>
      <command_interface name="effort"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
      <param name="p">2000</param>
      <param name="i">0</param>
      <param name="d">200</param>
    </joint>
    
    <joint name="panda_joint2">
      <command_interface name="position">
        <param name="min">-1.8326</param>
        <param name="max">1.8326</param>
